  "scripts": {
    "build": "tsc -b && webpack",
    "start": "tsc -b && webpack && wrangler pages dev web.out --binding API_PORT=3000 --binding API_PROTOCOL=http --binding API_DOMAIN=127.0.0.1 ",
    "test": "jest",
    "cdk": "cdk",
    "local-api": "cdk synth --quiet && sam local start-api -t ./cdk.out/TranmereWebStack.template.json",
    "scraper": "cdk synth && sam local invoke -t ./cdk.out/TranmereWebStack.template.json ScraperJobFunction4A8DAC15",
    "synth": "cdk synth --quiet",
    "acceptance-test": "newman run ./api_tests/postman-collection.json -e ./api_tests/env/aws-prod.json --env-var 'testApiKey=123'",
    "local-acceptance-test": "newman run ./api_tests/postman-collection.json -e ./api_tests/env/sam-local.json",
    "contentful-typescript-codegen": "contentful-typescript-codegen --output types/generated/contentful.d.ts",
    "prettier": "prettier '*.ts' 'lib/**/*.ts' 'lambda/**/*.ts' 'types/**/*.ts' 'test/**/*.ts' 'functions/**/*.ts' 'tranmere-web/assets/js/**/*.js' --write",
    "prepare": "husky"
  },
