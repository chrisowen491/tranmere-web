{
  "name": "tranmere-web",
  "version": "3.0.157",
  "description": "",
  "bin": {
    "tranmere-web": "bin/tranmere-web.mjs"
  },
  "engines": {
    "npm": ">=9.5.0",
    "node": ">=21.1.0"
