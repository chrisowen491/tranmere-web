{
  "name": "@tranmere-web/site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "site": "next dev --turbo -p 3001",
    "build": "next build",
    "postbuild": "next-sitemap",
    "lint": "eslint .",
    "start": "next start",
    "format": "prettier --write \"app\" --write \"lib\" --write \"components\"",
    "cloudflare-build": "npx opennextjs-cloudflare build",
    "cloudflare-deploy": "npx opennextjs-cloudflare deploy",
    "cloudflare-preview": "npx opennextjs-cloudflare build && npx opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "db": "npx wrangler d1 execute tranmere-web --local --file=../sql/schema.sql ",
    "db:remote": "npx wrangler d1 execute tranmere-web --remote --file=../sql/schema.sql"
  },
