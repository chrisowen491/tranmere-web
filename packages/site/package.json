{
  "name": "@tranmere-web/site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "site": "next dev --turbo -p 3001",
    "build": "next build",
    "postbuild": "next-sitemap",
    "lint": "eslint .",
    "start": "next start",
    "format": "prettier --write \"app\" --write \"lib\" --write \"components\"",
    "cloudflare-build": "npx @cloudflare/next-on-pages@1",
    "cloudflare-deploy": "npx wrangler pages deploy --branch=master",
    "cloudflare-preview": "npx wrangler pages dev --port 3001",
    "db": "npx wrangler d1 execute tranmere-web --local --file=../sql/schema.sql",
    "db:remote": "npx wrangler d1 execute tranmere-web --remote --file=../sql/schema.sql"
