<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="/assets/styles/tranmere-web.css?r={{random}}">

    <title>{{title}}</title>
    <meta name="title" content="{{title}} - Tranmere-Web">
    <meta name="google" content="notranslate">
    <meta name="description" content="{{{description}}}">
    <link rel="canonical" href="https://www.tranmere-web.com{{{url}}}">
    <script type="application/ld+json">
        {
            "@context": "https://schema.org",
            "@type": "{{pageType}}",
            "name": "{{title}} | Tranmere-Web",
            {{#headline}}"headline": "{{.}},{{/headline}}
            "url": "https://www.tranmere-web.com{{{url}}}",
            {{#datePosted}}"datePublished": "{{.}}",{{/datePosted}}
            "description": "{{{description}}}",
            {{#author}}"author": [{
                "@type": "Person",
                "name": "{{.}}",
                "url": "https://www.tranmere-web.com/about"
            }],{{/author}}
            {{#carousel.length}}
            "image": [
                {{#carousel}}
                "{{{imagePath}}}",
                {{/carousel}}
            ],
            {{/carousel.length}}
            {{^carousel.length}}
            "image": {
                "@type": "ImageObject",
                "url": "https://www.tranmere-web.com/assets/images/logo_v1.png",
                "width": 450,
                "height": 260
            }
            {{/carousel.length}}
        }
    </script>
</head>