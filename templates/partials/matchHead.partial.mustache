<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="/assets/styles/tranmere-web.css?r={{random}}" />

    <title>{{title}}</title>
    <meta name="title" content="{{title}} - Tranmere-Web" />
    <meta name="google" content="notranslate">
    <meta name="description" content="{{{description}}}">
    <link rel="canonical" href="https://www.tranmere-web.com{{{url}}}" />
    <script type="application/ld+json">
        {
            "@context": "https://schema.org",
            "@type": "{{pageType}}",
            "name": "{{title}} | Tranmere-Web",
            "url": "https://www.tranmere-web.com{{{url}}}",
            "description": "{{{description}}}",
            "image": {
                "@type": "ImageObject",
                "url": "https://www.tranmere-web.com/assets/images/logo_v1.png",
                "width": 450,
                "height": 260
            },
            "mainEntity": {
                "@type": "SportsEvent",
                "name": "{{home}} {{ft}} {{visitor}}",
                "awayTeam": {
                    "@type": "SportsTeam",
                    "name": "{{visitor}}"
                },
                "homeTeam": {
                    "@type": "SportsTeam",
                    "name": "{{home}}"
                },
                "sport": "Football",
                "eventStatus": "EventScheduled",
                {{#hasVenue}}"location": "{{venue}}",{{/hasVenue}}
                "startDate": "{{date}}",
                "description": "{{home}} {{ft}} {{visitor}}, {{competition}}, {{date}}",
                "url": "https://www.tranmere-web.com{{{url}}}",
                "superEvent": {
                    "name": "{{competition}}"
                },
                {{#largeProgramme}}
                "image": {
                    "@type": "ImageObject",
                    "url": "https://images.tranmere-web.com/{{largeProgramme}}",
                    "width": 310,
                    "height": 450
                }
                {{/largeProgramme}}
                {{^largeProgramme}}
                "image": {
                    "@type": "ImageObject",
                    "url": "https://www.tranmere-web.com/assets/images/logo_v1.png",
                    "width": 450,
                    "height": 260
                }
                {{/largeProgramme}}
            }
        }
    </script>
</head>