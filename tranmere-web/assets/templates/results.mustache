
<table class="table">
    <thead class="thead-dark">
    <tr>
        <th scope="col">Date</th>
        <th scope="col">Opposition</th>
        <th scope="col">Competition</th>
        <th scope="col">Result</th>
        <th scope="col">Att.</th>
        <th scope="col" class="d-none d-md-block">Programme</th>
    </tr>
    </thead>
    <tbody>
    {{#results}}
    <tr>
        <td>
            <a href="/match.html?date={{Date}}">{{Date}}</a>
        </td>
        <td>{{Opposition}} {{}}</td>
        <td>{{competition}}</td>
        <td>{{FT}}</td>
        <td>{{attendance}}</td>
        <td class="d-none d-md-block">{{#programme}}<img src="https://images.tranmere-web.com/{{programme}}"/>{{/programme}}</td>
    </tr>
    {{/results}}
    </tbody>
</table>
